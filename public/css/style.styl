html
  background-color: #fdfdf5
  
body
  background-color: #fdfdf5
  padding: 50px
  font-family:'Megrim', Helvetica, Arial, sans-serif
  margin-left: auto
  margin-right: auto
  
h1
    text-align: center
    color: #993366
    font-size: 60px
    
h2
    text-align:
    center

.panel-group#accordion
    margin: 0 auto
    max-width: 950px
    
.panel-heading#headingOne
    background-color: #6f8a79
    
.panel-heading#headingTwo
    background-color: #c86368
    
.panel-title
    text-align: center
    font-size: 30px
    color: #f5f5f5
    
.panel.panel-default
    background-color: #e5e6d8
    
.panel-body
    text-align: center

.linkedin-container-div
    margin: 0 auto
    position: relative 
    width: 275px
    height: 170px
    overflow: hidden
    
.linkedin-container-div iframe
    position: absolute
    top: -17px
    left: -12px